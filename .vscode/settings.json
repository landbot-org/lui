{
  "[markdown]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.markdownlint": "explicit"
    },
    "rewrap.autoWrap.enabled": true,
    "editor.wordWrap": "wordWrapColumn",
    "editor.wrappingIndent": "same",
    "editor.wordWrapColumn": 120,
    "editor.formatOnSave": true
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#78ea78",
    "activityBar.background": "#78ea78",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#7575ea",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#15202b99",
    "sash.hoverBorder": "#78ea78",
    "statusBar.background": "#4ce34c",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#22da22",
    "statusBarItem.remoteBackground": "#4ce34c",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#4ce34c",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#4ce34c99",
    "titleBar.inactiveForeground": "#15202b99"
  },
  "peacock.color": "#4ce34c"
}
